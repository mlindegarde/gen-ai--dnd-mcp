{
  "name": "q_ide_default",
  "description": "Default agent configuration",
  "prompt": "",
  "mcpServers": {
    "shinkuro": {
      "command": "uvx",
      "args": [
        "shinkuro"
      ],
      "env": {
        "FOLDER": "./.shinkuro/prompts",
        "VARIABLE_FORMAT": "dollar",
        "AUTO_DISCOVER_ARGS": "true",
        "SKIP_FRONTMATTER": "true"
      }
    }
  },
  "tools": [
    "fs_read",
    "execute_bash",
    "fs_write",
    "report_issue",
    "use_aws",
    "@shinkuro",
    "fsRead",
    "fsWrite",
    "fsReplace",
    "listDirectory",
    "fileSearch",
    "executeBash",
    "codeReview",
    "displayFindings"
  ],
  "toolAliases": {},
  "allowedTools": [
    "fs_read",
    "report_issue",
    "use_aws",
    "execute_bash",
    "fs_write",
    "fsRead",
    "listDirectory",
    "fileSearch",
    "codeReview",
    "displayFindings"
  ],
  "toolsSettings": {
    "use_aws": {
      "alwaysAllow": [
        {
          "preset": "readOnly"
        }
      ]
    },
    "execute_bash": {
      "alwaysAllow": [
        {
          "preset": "readOnly"
        }
      ]
    }
  },
  "resources": [
    "file://AmazonQ.md",
    "file://README.md",
    "file://.amazonq/rules/**/*.md"
  ],
  "hooks": {
    "agentSpawn": [],
    "userPromptSubmit": []
  },
  "useLegacyMcpJson": true
}